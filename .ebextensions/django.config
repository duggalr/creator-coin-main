option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "creator_coin_new.settings"
  aws:elasticbeanstalk:container:python:
    WSGIPath: creator_coin_new.wsgi:application  

container_commands:
  01_start:
    command: "echo starting initialization"
  02_makemigrations:
    command: "source /opt/python/run/venv/bin/activate && python creator_coin_new/manage.py makemigrations"
    leader_only: true
  02_migrate:
    command: "source /opt/python/run/venv/bin/activate && python creator_coin_new/manage.py migrate"
    leader_only: true
  03_collectstatic:
    command: "source /opt/python/run/venv/bin/activate && python creator_coin_new/manage.py collectstatic --noinput"
